{
    "name": "weather app",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "Weather in your city",

    "browser_action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
              "<all_urls>"
            ],
            "js": "main.js"
        }
    ]
}